{
  "name": "teste",
  "version": "6.14.2",
  "description": "teste",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rimraf ./dist",
    "copy-resources": "copyup 'src/**/*.{html,jpg,jpeg,png,css,json,wsdl,sql}' 'dist/'",
    "exec": "node --max_old_space_size=12288 dist/index.js",
    "exec:dev": "nodemon -r ts-node/register src/index.ts",
    "heroku:simulate-install": "rimraf node_modules && NPM_CONFIG_PRODUCTION=false NODE_ENV=production yarn install --pure-lockfile --ignore-engines",
    "job:sync": "yarn run exec job:sync",
    "lint": "tslint 'src/**/*.ts' -e 'src/**/*.test.ts'",
    "deploy:dev": "bash deploy.sh",
    "deploy": "serverless deploy",
    "makemigrations": "TEST=OK ts-node src/index.ts worker:make-migrations",
    "migrate": "yarn run exec worker:database-migration",
    "migrate:force": "yarn run exec worker:database-migration force",
    "migrate:force:test": "TEST=OK yarn run exec worker:database-migration force",
    "migrate:test": "TEST=OK yarn && TEST=OK yarn run exec worker:database-migration",
    "migration:new": "typeorm migration:create --dir src/common/data/db/setup/migration --name",
    "migrate:undo": "yarn run exec worker:database-migration undo",
    "notify": "osascript -e 'display notification \"is done!\" with title \"Your task...\"'",
    "build": "yarn run clean-dist && tsc && yarn copy-resources",
    "reinstall": "rimraf ./node_modules || true && yarn cache clean && yarn",
    "reset:test": "yarn build && yarn run migrate:force:test && yarn run seeds:test",
    "seeds": "yarn run exec worker:database-seed",
    "seeds:test": "TEST=OK yarn run exec worker:database-seed",
    "start": "$SCRIPT_TO_RUN",
    "test": "TS_NODE_TRANSPILE_ONLY=true TEST=OK nyc mocha --require ts-node/register --colors --timeout 30000 --delay src/index.ts --exit",
    "test:dev": "yarn run test --bail; yarn notify",
    "web": "nodemon -r ts-node/register src/index.ts web",
    "web:debug": "yarn run lint && nodemon --inspect -r ts-node/register src/index.ts web",
    "worker:example": "yarn run exec worker:example",
    "worker:csv-import": "yarn run exec worker:csv-import",
    "worker:update-reseller-uses-app": "yarn run exec worker:update-reseller-uses-app",
    "changelog:new": "bundle exec codelog new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dasda/asdasdasd.git"
  },
